<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="monday.com.random">
    
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-feature android:name="android.hardware.wifi" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginRegister.LoginRoute">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginRegister.Login">
        </activity>
        <activity android:name=".LoginRegister.SignUp">
        </activity>
        <activity android:name=".HomePage.HomeActivity">
        </activity>
        <activity android:name=".ArticleCreator.ArticleCreator">
        </activity>
        <activity android:name=".ArticleViewer.ViewArticle">
        </activity>
        <activity android:name=".Profile.Profile">
        </activity>
    </application>

</manifest>